#include <stdlib.h>
#include <stdio.h>

int check(int a, char b[int i])
{
	for(i = 0; i < argc; i++)
	{
		if(b[i] < 48 || b[i] > 97)
		{
			return (1);
		}
		else
		{
			return(check(a, b[i + 1]));
		}
	}
	return(0);
}


int main (int argc, char *argv[])
{
	int r,i;
	r = 0;
	int check = check(argc,argv[1]);

	if (check == 0)
	{
		for (i = 1; i < argc; i++)
		{
		r += atoi(argv[i]);
		}
	}

	printf("%i",r);
	return (r);
}
